<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="authInfoResolver" class="com.axiell.ehub.security.AuthInfoResolver"/>
    <bean class="com.axiell.ehub.security.EhubParamConverterProvider"/>

    <bean id="rootResource" class="com.axiell.ehub.RootResource">
        <property name="formatBusinessController" ref="issueBusinessController" />
        <property name="loanBusinessController" ref="loanBusinessController" />
    </bean>
</beans>
