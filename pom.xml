        <hibernate-tools.version>5.2.0.Alpha1</hibernate-tools.version>
        <hsqldb.version>2.2.9</hsqldb.version>
        <spring-data-jpa.version>1.10.2.RELEASE</spring-data-jpa.version>
        <hamcrest-all.version>1.3</hamcrest-all.version>
        <jodatime.version>2.1</jodatime.version>
        <commons-lang3.version>3.4</commons-lang3.version>
        <commons-codec.version>1.7</commons-codec.version>
        <commons.io.version>2.4</commons.io.version>
        <oracle.version>18.3.0.0</oracle.version>
        <jetty.version>9.3.0.M1</jetty.version>
        <jetty-maven-plugin.version>${jetty.version}</jetty-maven-plugin.version>
        <wiremock.version>2.6.0</wiremock.version>
        <logback.version>1.0.7</logback.version>
        <slf4j.version>1.7.2</slf4j.version>
        <wicket.version>1.4.23</wicket.version>
        <aspectj.version>1.6.12</aspectj.version>
        <joda-time.version>2.1</joda-time.version>
        <xmlunit.version>1.5</xmlunit.version>
        <liquibase.core.version>3.5.1</liquibase.core.version>
        <liquibase.slf4j.version>2.0.0</liquibase.slf4j.version>
        <jackson.version>2.8.11</jackson.version>
        <ehub.release.version>2.5.0.106</ehub.release.version>
        <ehub.snapshot.version>2.10-SNAPSHOT</ehub.snapshot.version>
        <jcabi-manifests.version>0.8.2</jcabi-manifests.version>
    </properties>

    <profiles>
        <profile>
            <id>jrebel</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.zeroturnaround</groupId>
                        <artifactId>jrebel-maven-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>generate-rebel-xml</id>
                                <phase>process-resources</phase>
                                <goals>
                                    <goal>generate</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
        <profile>
            <id>jacoco</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.jacoco</groupId>
                        <artifactId>jacoco-maven-plugin</artifactId>

                        <executions>
                            <execution>
                                <id>jacoco-initialize</id>
                                <goals>
                                    <goal>prepare-agent</goal>
                                </goals>
                            </execution>
                            <execution>
                                <id>jacoco-site</id>
                                <phase>install</phase>
                                <goals>
                                    <goal>report</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
        <profile>
            <id>source-install</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-source-plugin</artifactId>
                        <executions>
                            <execution>
                                <id>attach-sources</id>
                                <goals>
                                    <goal>jar</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
        <profile>
            <id>javadoc-install</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <build>
                <plugins>
                    <plugin>
                        <groupId>org.apache.maven.plugins</groupId>
                        <artifactId>maven-javadoc-plugin</artifactId>
                        <configuration>
                            <aggregate>false</aggregate>
                            <encoding>UTF-8</encoding>
                        </configuration>
                        <executions>
                            <execution>
                                <id>attach-javadocs</id>
                                <goals>
                                    <goal>jar</goal>
                                </goals>
                            </execution>
                        </executions>
                    </plugin>
                </plugins>
            </build>
        </profile>
    </profiles>
</project>
