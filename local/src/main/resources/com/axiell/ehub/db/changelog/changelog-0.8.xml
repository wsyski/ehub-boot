<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <property name="now" value="systimestamp" dbms="oracle"/>

    <changeSet id="1" author="ehub" dbms="oracle">
        <insert tableName="CONTENT_PROVIDER_ALIAS">
            <column name="ID" value="7"/>
            <column name="CREATE_DATETIME" valueComputed="${now}"/>
            <column name="MODIFY_DATETIME" valueComputed="${now}"/>
            <column name="ALIAS" value="DISTRIBUTÖR: ELIB"/>
            <column name="NAME" value="ELIB3"/>
        </insert>
        <comment>Adds 'DISTRIBUTÖR: ELIB' as ELIB3 alias</comment>
    </changeSet>

    <changeSet id="2" author="ehub" dbms="oracle">
        <insert tableName="CONTENT_PROVIDER_ALIAS">
            <column name="ID" value="8"/>
            <column name="CREATE_DATETIME" valueComputed="${now}"/>
            <column name="MODIFY_DATETIME" valueComputed="${now}"/>
            <column name="ALIAS" value="DISTRIBUTÖR: ATINGO"/>
            <column name="NAME" value="PUBLIT"/>
        </insert>
        <comment>Adds 'DISTRIBUTÖR: ATINGO' as PUBLIT alias</comment>
    </changeSet>

</databaseChangeLog>