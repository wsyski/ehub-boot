<?xml version="1.0"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="common-context.xml"/>

    <bean id="epResourceFactory" class="com.axiell.ehub.provider.ep.EpResourceFactory">
        <property name="httpEngine" ref="ehubHttpEngine"/>
    </bean>

    <bean id="borrowBoxResourceFactory" class="com.axiell.ehub.provider.borrowbox.BorrowBoxResourceFactory">
        <property name="httpEngine" ref="ehubHttpEngine"/>
    </bean>

</beans>
