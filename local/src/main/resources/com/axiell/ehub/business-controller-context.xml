<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.1.xsd">

	<import resource="persistence-context.xml"/>

	<!-- List only the Business Controllers here -->
	<bean id="consumerBusinessController" class="com.axiell.ehub.consumer.ConsumerBusinessController"/>
	<bean id="recordBusinessController" class="com.axiell.ehub.lms.record.RecordBusinessController"/>
	<bean id="formatBusinessController" class="com.axiell.ehub.provider.record.format.FormatBusinessController"/>
	<bean id="loanBusinessController" class="com.axiell.ehub.loan.LoanBusinessController"/>
</beans>